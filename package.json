{
  "name": "@sgpinkus/keycloak-js",
  "version": "1.0.0",
  "description": "A simple keycloak javascript adapter",
  "author": "sgpinkus",
  "license": "MIT",
  "keywords": [
    "keycloak"
  ],
  "homepage": "https://github.com/sgpinkus/keycloak-js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sgpinkus/keycloak-js"
  },
  "scripts": {
    "_build-tsc-watch": "tsc -w",
    "_build-tsc": "tsc",
    "_build-watch": "rollup -w --config rollup.config.mjs",
    "_build": "rollup --config rollup.config.mjs",
    "build": "npm-run-all clean _build",
    "clean": "rm -rf dist/*",
    "dev": "npm-run-all clean -p _build-watch",
    "lint": "eslint src/",
    "prepare": "npm run build",
    "test": "TS_NODE_PROJECT=tsconfig.node.json ts-node src/test.ts"
  },
  "type": "module",
  "main": "dist/index.mjs",
  "browser": "dist/index.min.js",
  "types": "dist/types",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.cjs",
    "browser": "./dist/index.min.js"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "base64-js": "^1.5.1",
    "js-sha256": "^0.9.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.2",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^11.1.2",
    "@types/node": "^18.11.18",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "eslint": "^8.44.0",
    "nodemon": "^2.0.22",
    "npm-run-all": "^4.1.5",
    "rollup": "^3.26.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.0",
    "typescript": "^4.9.5"
  }
}
